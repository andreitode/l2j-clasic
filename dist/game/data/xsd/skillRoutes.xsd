<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="list">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="skillRoutes">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="skillRoute" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="skillId" type="xs:int"/>
                                        <xs:element name="routeId" type="xs:int"/>
                                        <xs:element name="name" type="xs:string"/>
										<xs:element name="description" type="xs:string"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>